@model IEnumerable<Job_Offers_Website.Models.Job>

@{
    ViewBag.Title = "RequestPost";
}

<h2>RequestPost</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
